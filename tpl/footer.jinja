{% set data = getFooterData() %}
<footer class="px-4 pt-4 pb-1 mt-4 bg-white custom-footer">
    <div class="d-flex justify-content-center">
    {% for observatory in data.observatories %}
        <img src="{{getThumborUrl('x100', observatory.logo)}}" alt="{{observatory.title}}" class="observatory-logo"/>    
    {% endfor %}
    </div>
    <div class="mt-4 d-flex">
        {% include 'components/legal-footer.jinja' %}
    </div>
</footer>